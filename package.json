  "scripts": {
    "prepare": "husky",
    "start": "vite dev --host",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "depcheck": "depcheck --skip-missing=true",
    "audit": "npm audit --omit=dev || true",
    "semgrep": "semgrep scan --error --config p/ci",
    "semgrep:local": "semgrep scan --config .semgrep",
    "pact:test": "jest pact",
    "cypress:ci": "echo 'Cypress not installed'",
    "env:verify": "node scripts/verify-env.js",
    "openapi:gen": "openapi-typescript ./openapi.yaml -o ./src/types/api.d.ts || echo 'OpenAPI file not found'",
    "check:types": "tsc -p tsconfig.json --noEmit",
    "check:build": "vite build",
    "check:lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings 0",
    "check:format": "prettier --check .",
    "check:test": "jest --coverage --runInBand",
    "check:accessibility": "jest --runInBand --testPathPattern=a11y.test",
    "check:deadcode": "ts-prune --ignore 'src/**/__tests__/**'",
    "check:deps": "knip",
    "check:colors": "powershell -ExecutionPolicy Bypass -File scripts/check-colors.ps1",
    "check:env": "node scripts/validate-env.cjs",
    "check:openapi": "node scripts/validate-openapi.cjs || exit 0",
    "check:all": "npm-run-all -l check:env check:types check:lint check:format check:test check:accessibility check:deadcode check:deps check:colors check:build",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "orphan:check": "node scripts/find-orphan-pages.js",
    "orphan:prune": "node scripts/prune-orphan-pages.js",
    "migrate:components": "node scripts/migrate-components.js",
    "find:unused": "node scripts/find-unused-components.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:a11y": "node scripts/test-a11y.js",
    "test:a11y:coverage": "node scripts/test-a11y.js --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
